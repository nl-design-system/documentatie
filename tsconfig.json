{
  "extends": "@docusaurus/tsconfig",
  "compilerOptions": {
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "skipLibCheck": true,
    "verbatimModuleSyntax": true,
    "paths": {
      "@docs/*": ["./docs/*"],
      "@site/*": ["./*"],
      "@theme/*": ["./src/theme/*"],
      "@docusaurus/*": ["./src/docusaurus-alternative/*"],
      "@docusaurus/useDocusaurusContext": ["./src/docusaurus-alternative/useDocusaurusContext"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", "**/*.mdx", "*.ts"],
  "exclude": ["**/node_modules/*", ".docusaurus/", "build/", "dist/", "packages/"]
}
