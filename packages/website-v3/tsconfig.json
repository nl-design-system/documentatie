{
  "extends": "../../tsconfig.build.json",
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": "../../",
    "rootDir": "../../",
    "jsx": "react-jsx",
    "paths": {
      "@docs/*": ["./docs/*"],
      "@site/*": ["./*"],
      "@theme/*": ["./src/theme/*"],
      "@docusaurus/*": ["./src/docusaurus-alternative/*"]
    }
  },
  "files": ["../../.docusaurus/registry.js", "../../.docusaurus/routes.js"],
  "include": [
    "../../**/*.ts",
    "../../**/*.tsx",
    "../../**/*.mdx",
    "../../*.ts",
    "../../**/*.mdx",
    "../../.docusaurus/*.js",
    "../../.docusaurus/*.mjs",
    "../../.docusaurus/**/*.json"
  ],
  "exclude": ["**/node_modules/*"]
}
