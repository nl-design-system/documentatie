{
  "name": "@nl-design-system/website",
  "version": "0.0.0",
  "private": true,
  "license": "EUPL-1.2",
  "packageManager": "pnpm@8.12.1",
  "engines": {
    "node": "^20",
    "pnpm": "^8"
  },
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "npm-run-all --sequential write-component-pages start:docusaurus",
    "start:docusaurus": "docusaurus start",
    "build": "npm-run-all --sequential write-component-pages build:workspaces build:docusaurus build:optimize-view-source",
    "build:component-pages": "docusaurus build",
    "build:docusaurus": "docusaurus build",
    "build:workspaces": "lerna run build",
    "build:optimize-view-source": "cd build && npx prettier --config ../.prettierrc.build.json --no-editorconfig --write .",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clean": "npm-run-all clean:**",
    "clean:docusaurus": "docusaurus clear",
    "clean:playwright": "playwright uninstall",
    "clean:generated": "rimraf build/ dist/ tmp/",
    "serve": "http-server --port 3000 build/",
    "serve:test-report": "pnpm exec playwright show-report tmp/playwright-html-report",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "write-component-pages": "ts-node --project ./tsconfig.commonjs.json src/_generate-component-pages",
    "typecheck": "tsc",
    "lint": "npm-run-all --continue-on-error lint:**",
    "lint-todo:css": "stylelint --allow-empty-input '**/*.{css,scss}'",
    "lint:js": "eslint --ext '.js,.json,.jsx,.mdx,.ts,.tsx,.vue' --report-unused-disable-directives .",
    "lint:prettier": "prettier --check .",
    "lint-todo:ts": "tsc --project tsconfig.json --noEmit",
    "lint-fix": "npm-run-all --continue-on-error lint-fix:**",
    "lint-fix-todo:css": "stylelint --fix '**/*.{css,scss}'",
    "lint-fix:js": "eslint --ext '.js,.json,.jsx,.mdx,.ts,.tsx' --fix --report-unused-disable-directives .",
    "lint-fix:prettier": "prettier --write .",
    "publish": "lerna publish from-package --yes",
    "release": "lerna version prerelease --conventional-prerelease --no-changelog --no-private --yes",
    "update-patch": "npm-check-updates --configFileName .ncurc.patch.cjs --deep --dep dev,prod --target patch --upgrade && pnpm install",
    "update-minor": "npm-check-updates --configFileName .ncurc.minor.cjs --deep --dep dev,prod --target minor --upgrade && pnpm install",
    "update-major": "npm-check-updates --configFileName .ncurc.major.cjs --deep --dep dev,prod --target latest --upgrade && pnpm install",
    "install-test-browsers": "playwright install",
    "test-e2e": "playwright test test/e2e/",
    "test-visual": "playwright test test/visual/",
    "publish:argos": "argos upload ./tmp/screenshots",
    "test-update": "npm-run-all --sequential clean lint build"
  },
  "dependencies": {
    "@docusaurus/core": "3.0.1",
    "@docusaurus/preset-classic": "3.0.1",
    "@docusaurus/theme-common": "3.0.1",
    "@docusaurus/theme-search-algolia": "3.0.1",
    "@tabler/icons-react": "^2.44.0",
    "@utrecht/design-tokens": "1.0.0-alpha.613",
    "@utrecht/web-component-library-react": "1.0.2",
    "clsx": "2.0.1",
    "html-react-parser": "^5.0.11",
    "iframe-resizer-react": "1.1.0",
    "prism-react-renderer": "2.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@argos-ci/cli": "1.0.5",
    "@argos-ci/playwright": "1.6.0",
    "@docsearch/react": "3.5.2",
    "@docusaurus/module-type-aliases": "3.0.1",
    "@docusaurus/plugin-content-blog": "3.0.1",
    "@docusaurus/plugin-content-docs": "3.0.1",
    "@docusaurus/theme-classic": "3.0.1",
    "@docusaurus/tsconfig": "3.0.1",
    "@docusaurus/types": "3.0.1",
    "@fontsource/fira-sans": "5.0.18",
    "@fontsource/open-sans": "^5.0.20",
    "@fontsource/source-sans-pro": "5.0.8",
    "@lerna-lite/cli": "3.1.0",
    "@lerna-lite/publish": "3.1.0",
    "@lerna-lite/run": "3.1.0",
    "@lerna-lite/version": "3.1.0",
    "@mdx-js/react": "3.0.0",
    "@nl-design-system-unstable/nlds-design-tokens": "workspace:*",
    "@nl-design-system-unstable/voorbeeld-design-tokens": "1.0.0-alpha.109",
    "@nl-design-system/component-index": "1.0.0-alpha.16",
    "@playwright/test": "1.40.1",
    "@types/node": "20.10.6",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "@utrecht/component-library-css": "3.0.0",
    "@utrecht/component-library-react": "3.0.0",
    "cheerio": "1.0.0-rc.12",
    "eslint": "8.49.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-mdx": "2.2.1",
    "eslint-plugin-react": "7.33.2",
    "firacode": "6.2.0",
    "http-server": "14.1.1",
    "npm-check-updates": "16.14.12",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.8",
    "react-hook-form": "^7.49.2",
    "react-player": "2.13.0",
    "react-syntax-highlighter": "^15.5.0",
    "rimraf": "5.0.5",
    "stylelint": "15.11.0",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-standard-scss": "11.1.0",
    "stylelint-order": "6.0.4",
    "tabler-icons-react": "^1.56.0",
    "ts-node": "10.9.2",
    "typescript": "5.3.3",
    "uuid": "9.0.1",
    "yaml-loader": "^0.8.0"
  },
  "overrides": {
    "@docusaurus/core": {
      "react": "^16 || ^17 || ^18",
      "react-dom": "^16 || ^17 || ^18"
    },
    "@docusaurus/preset-classic": {
      "react": "^16 || ^17 || ^18",
      "react-dom": "^16 || ^17 || ^18"
    },
    "flux": {
      "react": "^16 || ^17 || ^18",
      "react-dom": "^16 || ^17 || ^18"
    },
    "mini-create-react-context": {
      "react": "^16 || ^17 || ^18",
      "react-dom": "^16 || ^17 || ^18"
    }
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
