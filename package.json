    "@mdx-js/react": "^1.6.21",
    "clsx": "^1.1.1",
    "prism-react-renderer": "^1.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^2.0.0-beta.15",
    "@docusaurus/types": "^2.0.0-beta.15",
    "@tsconfig/docusaurus": "^1.0.4",
    "@types/node": "^17.0.18",
    "mdx-embed": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.5.1",
    "ts-node": "10.8.2",
    "typescript": "^4.5.5"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
